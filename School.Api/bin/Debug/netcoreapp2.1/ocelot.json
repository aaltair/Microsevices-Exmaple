{
  "ReRoutes": [
    {
      "DownstreamPathTemplate": "/swagger/v1/swagger.json",
      "DownstreamScheme": "http",
      "ServiceName": "courses",
      "UpstreamPathTemplate": "/Courses/swagger.json",
      "UpstreamHttpMethod": [ "Get", "POST" ]

    },
    {
      "DownstreamPathTemplate": "/swagger/v1/swagger.json",
      "DownstreamScheme": "http",
      "ServiceName": "identity",
      "UpstreamPathTemplate": "/identity/swagger.json",
      "UpstreamHttpMethod": [ "Get", "POST" ]

    },
    {
      "DownstreamPathTemplate": "/api/{API}",
      "DownstreamScheme": "http",
      "ServiceName": "courses",
      "UpstreamPathTemplate": "/courses-api/{API}",
      "UpstreamHttpMethod": [ "Get", "Post", "Put", "Delete" ],
      "LoadBalancerOptions": {
        "Type": "RoundRobin"
      },
      "AuthenticationOptions": {
        "AuthenticationProviderKey": "TestKey",
        "AllowedScopes": []
      }



    },
    {
      "DownstreamPathTemplate": "/api/{API}",
      "DownstreamScheme": "http",
      "ServiceName": "identity",
      "UpstreamPathTemplate": "/identity-api/{API}",
      "UpstreamHttpMethod": [ "Get", "Post", "Put", "Delete" ]


    }
  ],
  "GlobalConfiguration": {
    "ServiceDiscoveryProvider": {
      "Host": "localhost",
      "Port": 8500,
      "Type": "Consul"
    }
  }
}